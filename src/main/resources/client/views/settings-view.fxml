<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.SettingsController">
   <children>
      <CheckBox fx:id="onlineBox" layoutX="71.0" layoutY="161.0" mnemonicParsing="false" onAction="#onlineMarked" text="online" />
      <TextField fx:id="hostTextField" layoutX="61.0" layoutY="49.0" onAction="#hostTyped" />
      <TextField fx:id="portTextField" layoutX="61.0" layoutY="111.0" onAction="#portTyped" />
      <Button fx:id="saveButton" layoutX="298.0" layoutY="78.0" mnemonicParsing="false" onAction="#savePressed" prefHeight="25.0" prefWidth="57.0" text="save" />
      <Button fx:id="backButton" layoutX="298.0" layoutY="157.0" mnemonicParsing="false" onAction="#quitPressed" prefHeight="25.0" prefWidth="57.0" text="back" />
      <CheckBox fx:id="hostBox" layoutX="150.0" layoutY="161.0" mnemonicParsing="false" onAction="#hostMarked" text="host" />
      <Text layoutX="61.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="host" />
      <Text layoutX="61.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="port" />
      <ChoiceBox fx:id="playerCountChoice" layoutX="61.0" layoutY="204.0" prefWidth="150.0" />
      <Text fx:id="settingsSavedText" layoutX="289.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="settings saved" />
   </children>
</AnchorPane>
